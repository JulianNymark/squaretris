class GameGrid {
  int w, h;
  
  int[][] grid;
  
  GameGrid(int w, int h){
    this.w = w;
    this.h = h;
    
    grid = new int[w][h];
    
  }
  
  public void draw(){
    //noStroke();
    fill(255);
    
    for (int x=0; x<w; ++x){
      for (int y=0; y<h; ++y){
        rect(x, y, 20, 20);
      }
    }
  }
}
